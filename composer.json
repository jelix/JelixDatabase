{
    "name": "jelix/database",
    "type": "library",
    "description": "Api implementing access to SQL databases",
    "keywords": ["database", "sql"],
    "homepage": "http://jelix.org",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Laurent Jouanneau",
            "email": "laurent@jelix.org"
        }
    ],
    "require": {
        "php": ">=7.2",
        "jelix/profiles": "^1.0.0",
        "psr/log": "^1.1.2"
    },
    "require-dev" : {
        "phpunit/phpunit": "8.5.*"
    },
    "archive": {
        "exclude": ["/tests"]
    },
    "autoload": {
        "psr-4" : {
            "Jelix\\Database\\" : "src/"
        }
    },
    "minimum-stability": "stable"
}
